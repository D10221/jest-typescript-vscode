{
  "name": "jest-typescript-vscode",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/jest.cmd",
    "build": "tsc -p .",
    "clean": "rimraf ./built",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {    
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "@types/jest": "^16.0.1",
    "@types/node": "^6.0.52",
    "@types/react": "^0.14.55",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.19",
    "jest": "^18.0.0",
    "jest-cli": "^18.0.0",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "noJest":{
    "testRegex": ".*\\.test\\.(ts|tsx|js)$",
     "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [      
      "js"
    ],    
    "testRegex": ".*\\.test\\.js$"
  }
}
